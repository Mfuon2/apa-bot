{
  "name": "sap_bone",
  "version": "0.0.1",
  "description": "A guide to REST API using Typescript",
  "main": "app.ts",
  "scripts": {
    "start": "tsc && nodemon app.ts",
    "build": "tsc",
    "pre-commit": "lint-staged",
    "debug": "export DEBUG=* && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --check . && eslint src/**/*.ts",
    "format": "prettier --write . && eslint src/**/*.ts --fix",
    "prepare": "husky install && npx husky add .husky/pre-commit \"yarn run lint-fix\"",
    "lint-fix": "eslint src/**/*.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfuon2/sap_bone.git"
  },
  "keywords": [
    "REST",
    "API",
    "ExpressJS",
    "NodeJS"
  ],
  "author": "Marcos Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mfuon2/sap_bone/issues"
  },
  "homepage": "https://github.com/mfuon2/sap_bone#readme",
  "dependencies": {
    "@types/morgan": "^1.9.3",
    "argon2": "^0.28.2",
    "async-redis": "^2.0.0",
    "axios": "^0.21.3",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-winston": "^4.0.5",
    "flatted": "^3.2.2",
    "g": "^2.0.1",
    "googleapis": "^67.1.0",
    "https": "^1.0.0",
    "husky": "^7.0.2",
    "ioredis": "^4.27.8",
    "json-server": "^0.16.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "shortid": "^2.2.16",
    "twilio": "^3.67.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.26.6",
    "@types/redis": "^2.8.31",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^11.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.16",
    "tslint": "^6.0.0",
    "typescript": "^4.3.5"
  }
}
